{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "require": {
        "php": ">=7.4.0",
        "ext-bcmath": "*",
        "ext-dom": "*",
        "ext-json": "*",
        "ext-curl": "*",
        "ext-openssl": "*",
        "ext-redis": "*",
        "godruoyi/php-snowflake": "^2.0",
        "zoujingli/ip2region": "^2.0"
    },
    "require-dev": {
        "roave/security-advisories": "dev-latest",
        "phpunit/phpunit": "^9.3.3"
    },
    "autoload": {
        "files": [
            "src/dddCore/Supports/helpers.php"
        ],
        "psr-4": {
            "DDDCore\\": "src/dddCore"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    }
}
