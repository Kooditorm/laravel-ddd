{
  "name": "kooditorm/laravel-ddd",
  "description": "The Core Package of Domain Driven Architecture Based on Laravel System",
  "keywords": [
    "Kooditorm",
    "laravel",
    "laravel-dddCore",
    "ddd",
    "Domain Driven Design"
  ],
  "homepage": "https://github.com/Kooditorm/laravel-ddd",
  "license": "MIT",
  "author": [
    {
      "name": "oswin hu",
      "email": "oswin-hu@gmail.com"
    }
  ],
  "require": {
    "php": ">=7.4.0",
    "ext-json": "*",
    "laravel/framework": "^7.0|^8.0|^9.0",
    "laravel/tinker": "^2.0",
    "tymon/jwt-auth": "^1.0",
    "prettus/l5-repository": "^2.7",
    "egulias/email-validator": " ^2.1.10",
    "doctrine/lexer": "^1.2.3",
    "doctrine/annotations": "^1.13"
  },
  "require-dev": {
    "roave/security-advisories": "dev-latest",
    "phpunit/phpunit": "^9.3.3"
  },
  "autoload": {
    "files": [
      "src/Supports/helpers.php"
    ],
    "psr-4": {
      "DDDCore\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "DDDCore\\Providers\\DDDCoreServiceProvider",
        "DDDCore\\Providers\\ListenDBServiceProvider"
      ]
    }
  }
}
